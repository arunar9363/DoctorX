{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "INFERMEDICA_APP_ID": "@infermedica_app_id",
    "INFERMEDICA_APP_KEY": "@infermedica_app_key",
    "REACT_APP_RSS2JSON_API_KEY": "@react_app_rss2json_api_key",
    "REACT_APP_WHO_API_BASE": "https://www.who.int/api",
    "REACT_APP_WHO_RSS_BASE": "https://www.who.int/rss-feeds",
    "REACT_APP_RSS2JSON_URL": "https://api.rss2json.com/v1/api.json"
  },
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@18"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}